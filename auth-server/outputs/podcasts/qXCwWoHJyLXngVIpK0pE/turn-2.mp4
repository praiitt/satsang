        const APP = isDevelopment ? DEV_APP_URL : APP_HEYGEN_COM_URL;
        const MOBILE_WEB = isDevelopment ? DEV_MOBILE_WEB_URL : MOBILE_WEB_URL;

        if ((!isMwebCookieTrue || !isMobileDevice) && isMobileWebBundle) {
          window.location.replace(APP);
